{
    "name": "fadlee/mini-s3",
    "description": "A lightweight S3-compatible object storage server implemented in PHP",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "bcosca/fatfree-core": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('config/config.php') || copy('config/config.example.php', 'config/config.php');\""
        ]
    }
}
